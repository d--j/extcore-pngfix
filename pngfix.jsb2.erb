{
    projectName: 'extcore-pngfix',
    deployDir: 'deploy/pngfix-<%= version_string %>',
    licenseText: "extcore-pngfix. PNG with alpha transparency for IE6. Copyright 2009 Daniel Jagszent. MIT License.",
    pkgs: [{
        name: 'PngFix',
        file: 'pngfix.js',
        isDebug: true,
        fileIncludes: [{
            text: 'pngfix.js',
            path: 'src/'
        }]
    }],
    resources: [{
        src: 'src/',
        dest: 'src/',
        filters: ".*\\.js"
    },{
        src: 'test/',
        dest: 'test/',
        filters: ".*[\\.html|\\.jpg|\\.png|\\.gif|\\.css|\\.js]"
    },{
		src: 'doc/',
		dest: 'doc/',
		filters: '.*'
	},{
        src: 'license.txt',
        dest: 'license.txt',
        filters: ".*"
    },{
        src: 'README.markdown',
        dest: 'README.markdown',
        filters: ".*"
    }]
}

